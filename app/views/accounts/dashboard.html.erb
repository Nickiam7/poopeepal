<main style="height: calc(100vh - 155px);">
<div class="container">
   <div class="row">
      <div class="col">
         <div class="card">
            <div class="card-body box-shadow">
               <h1><%= @account.username %></h1>
               <% @babies.each do |baby| %>
                  <%= link_to baby.name, baby_path(baby) %>
               <% end %>
               <%= link_to "Create New Baby Dashboard", new_baby_path %> <br>            
            </div>
         </div>
   </div>
</div>
</main>